<% provide(:title, "All Users") %>

<h1>All Users</h1>

<table>
  <thead>
    <tr>
      <td>User Name</td>
      <td>Email</td>
      <td>Password</td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr class="user-row" id="<%= dom_id(user) %>">
        <td class="user_name"><%= user.user_name %></td>
        <td class="email"><%= user.email %></td>
        <td class="password"><%= user.password %></td>
      </tr>
    <% end %>
  </tbody>
</table>